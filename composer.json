{
	"name": "algoritmika/discussions-tab-for-woocommerce-products",
	"description": "Creates a discussions tab in WooCommerce Products",
	"keywords": [
		"wordpress",
		"plugin",
		"woocommerce",
		"discussion",
		"products",
		"comments",
		"tab"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomas Anbinderis",
			"email": "anbinderis@gmail.com"
		},
		{
			"name": "Pablo Pacheco",
			"email": "pablo.sg.pacheco@gmail.com"
		}
	],
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true
	},
	"type": "wordpress-plugin",
	"extra": {
		"installer-paths": {
			"vendor/{$vendor}/{$name}": [
				"type:wordpress-plugin",
				"type:wordpress-muplugin"
			]
		}
	},
	"autoload": {
		"classmap": [
			"classes/"
		]
	},
	"require": {
		"php": ">=5.3.2",
		"composer/installers": ">=v1.2.0",
		"pablo-pacheco/wc-custom-settings-options": "dev-master"
	}
}